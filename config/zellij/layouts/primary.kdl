layout {
    default_tab_template {
        pane size=1 borderless=true {
            plugin location="tab-bar"
        }
        children
        pane size=1 borderless=true {
            plugin location="status-bar"
        }
    }
    tab name="btop" {
        pane command="btop"
    }
    tab name="fastfetch" {
        pane command="fastfetch"
        pane
    }
    tab name="fastfetch-2" {
        pane command="fastfetch"
        pane
    }
}
