[
  // ===================================================================
  // TERMINAL OPERATIONS
  // ===================================================================
  {
    "key": "ctrl+enter",
    "command": "workbench.action.terminal.sendSequence",
    "when": "terminalFocus",
    "args": {
      "text": "\\\r\n"
    }
  },
  {
    "key": "shift+enter",
    "command": "workbench.action.terminal.sendSequence",
    "args": {
      "text": "\\\r\n"
    },
    "when": "terminalFocus"
  },
  {
    "key": "shift+ctrl+t",
    "command": "workbench.action.terminal.toggleTerminal"
  },
  {
    "key": "ctrl+h",
    "command": "workbench.action.terminal.kill"
  },
  {
    "key": "ctrl+shift+alt+5",
    "command": "workbench.action.terminal.split",
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+u",
    "command": "workbench.action.terminal.new",
    "when": "!terminalFocus"
  },
  {
    "key": "ctrl+u",
    "command": "workbench.action.terminal.newInActiveWorkspace"
  },
  {
    "key": "ctrl+b .",
    "command": "workbench.action.quickOpenTerm",
    "when": "!terminalFocus"
  },
  {
    "key": "ctrl+b r",
    "command": "workbench.action.terminal.rename",
    "when": "!terminalFocus"
  },
  {
    "key": "ctrl+y 1",
    "command": "workbench.action.terminal.focusAtIndex1"
  },
  {
    "key": "ctrl+y 2",
    "command": "workbench.action.terminal.focusAtIndex2"
  },
  {
    "key": "ctrl+y 3",
    "command": "workbench.action.terminal.focusAtIndex3"
  },
  {
    "key": "ctrl+y 4",
    "command": "workbench.action.terminal.focusAtIndex4"
  },
  {
    "key": "ctrl+y 5",
    "command": "workbench.action.terminal.focusAtIndex5"
  },
  {
    "key": "ctrl+y 6",
    "command": "workbench.action.terminal.focusAtIndex6"
  },
  {
    "key": "ctrl+y 7",
    "command": "workbench.action.terminal.focusAtIndex7"
  },
  {
    "key": "ctrl+y 8",
    "command": "workbench.action.terminal.focusAtIndex8"
  },
  {
    "key": "ctrl+y 9",
    "command": "workbench.action.terminal.focusAtIndex9"
  },
  // ===================================================================
  // NAVIGATION & EDITOR MANAGEMENT
  // ===================================================================
  {
    "key": "alt+ctrl+left",
    "command": "workbench.action.previousEditor"
  },
  {
    "key": "alt+ctrl+right",
    "command": "workbench.action.nextEditor"
  },
  {
    "key": "alt+ctrl+left",
    "command": "workbench.action.previousPanelView",
    "when": "panelFocus"
  },
  {
    "key": "alt+ctrl+right",
    "command": "workbench.action.nextPanelView",
    "when": "panelFocus"
  },
  {
    "key": "alt+ctrl+left",
    "command": "workbench.action.previousSideBarView",
    "when": "sideBarFocus"
  },
  {
    "key": "alt+ctrl+right",
    "command": "workbench.action.nextSideBarView",
    "when": "sideBarFocus"
  },
  {
    "key": "ctrl+alt+left",
    "command": "workbench.action.previousAuxiliaryBarView",
    "when": "auxiliaryBarVisible && !sideBarFocus && !panelFocus && !editorFocus"
  },
  {
    "key": "ctrl+alt+right",
    "command": "workbench.action.nextAuxiliaryBarView",
    "when": "auxiliaryBarVisible && !sideBarFocus && !panelFocus && !editorFocus"
  },
  {
    "key": "ctrl+alt+left",
    "command": "workbench.action.previousEditor",
    "when": "editorFocus"
  },
  {
    "key": "ctrl+alt+right",
    "command": "workbench.action.nextEditor",
    "when": "editorFocus"
  },
  {
    "key": "ctrl+alt+left",
    "command": "workbench.action.previousPanelView",
    "when": "panelFocus"
  },
  {
    "key": "ctrl+alt+right",
    "command": "workbench.action.nextPanelView",
    "when": "panelFocus"
  },
  {
    "key": "ctrl+alt+left",
    "command": "workbench.action.previousSideBarView",
    "when": "sideBarFocus"
  },
  {
    "key": "ctrl+alt+right",
    "command": "workbench.action.nextSideBarView",
    "when": "sideBarFocus"
  },
  {
    "key": "ctrl+alt+down",
    "command": "workbench.action.nextSideBarView",
    "when": "sideBarFocus"
  },
  {
    "key": "ctrl+alt+up",
    "command": "workbench.action.previousSideBarView",
    "when": "sideBarFocus"
  },
  // ===================================================================
  // WINDOW & UI MANAGEMENT
  // ===================================================================
  {
    "key": "ctrl+r",
    "command": "workbench.action.reloadWindow",
    "when": ""
  },
  {
    "key": "ctrl+=",
    "command": "workbench.action.zoomIn"
  },
  {
    "key": "ctrl+-",
    "command": "workbench.action.zoomOut"
  },
  {
    "key": "ctrl+0",
    "command": "workbench.action.zoomReset"
  },
  {
    "key": "ctrl+j",
    "command": "workbench.action.togglePanel"
  },
  {
    "key": "ctrl+b",
    "command": "workbench.action.toggleSidebarVisibility"
  },
  {
    "key": "ctrl+m",
    "command": "workbench.action.toggleMaximizedPanel"
  },
  {
    "key": "shift+ctrl+;",
    "command": "workbench.action.toggleZenMode"
  },
  // ===================================================================
  // GIT & SOURCE CONTROL
  // ===================================================================
  {
    "key": "shift+ctrl+g shift+ctrl+g",
    "command": "workbench.view.scm"
  },
  {
    "key": "shift+ctrl+g shift+ctrl+h",
    "command": "workbench.view.extension.github-pull-requests"
  },
  {
    "key": "shift+ctrl+g shift+ctrl+h",
    "command": "pr:github.focus"
  },
  {
    "key": "shift+ctrl+g shift+ctrl+p",
    "command": "pr.create"
  },
  {
    "key": "shift+ctrl+g shift+ctrl+o",
    "command": "pr.openPullRequestInGitHub"
  },
  {
    "key": "shift+ctrl+g shift+ctrl+r",
    "command": "pr.refreshChanges"
  },
  {
    "key": "shift+ctrl+g shift+ctrl+c",
    "command": "pr.pick"
  },
  // ===================================================================
  // PROJECT MANAGEMENT
  // ===================================================================
  {
    "key": "shift+ctrl+l",
    "command": "projectManager.listProjects"
  },
  {
    "key": "shift+ctrl+i",
    "command": "projectManager.listProjectsNewWindow"
  },
  // ===================================================================
  // WORKSPACE MANAGEMENT
  // ===================================================================
  {
    "key": "shift+alt+ctrl+l",
    "command": "vscodeWorkspaceSwitcher.openWorkspace"
  },
  {
    "key": "shift+alt+ctrl+i",
    "command": "vscodeWorkspaceSwitcher.openWorkspaceInNewWindow"
  },
  // ===================================================================
  // TEXT EDITING & SELECTION
  // ===================================================================
  {
    "key": "alt+ctrl+l",
    "command": "editor.action.selectHighlights",
    "when": "editorFocus"
  },
  // ===================================================================
  // TAB MANAGEMENT
  // ===================================================================
  {
    "key": "shift+ctrl+left",
    "command": "movetab.moveTabToLeft"
  },
  {
    "key": "shift+ctrl+right",
    "command": "movetab.moveTabToRight"
  },
  {
    "key": "shift+ctrl+up",
    "command": "movetab.moveTabToFirst"
  },
  {
    "key": "shift+ctrl+down",
    "command": "movetab.moveTabToLast"
  },
  // ===================================================================
  // AI & CHAT FEATURES
  // ===================================================================
  {
    "key": "ctrl+.",
    "command": "workbench.action.chat.toggleAgentMode",
    "when": "chatIsEnabled && inChat && chatLocation == 'panel'"
  },
  // ===================================================================
  // AI FEATURES
  // ===================================================================
  {
    "key": "shift+ctrl+-",
    "command": "workbench.action.openChat.copilot"
  },
  {
    "key": "ctrl+n",
    "command": "composer.createNewComposerTab",
    "when": "composerFocused || activeEditor == 'workbench.editor.composer' || focusedView =~ /^workbench.panel.aichat.view/"
  },
  {
    "key": "ctrl+n",
    "command": "antigravity.prioritized.chat.openNewConversationFromTerminal",
    "when": "terminalFocus"
  },
  // ===================================================================
  // DOCUMENTATION & CODE GENERATION
  // ===================================================================
  {
    "key": "ctrl+alt+a ctrl+alt+a",
    "command": "docthis.documentThis"
  },
  {
    "key": "ctrl+alt+z ctrl+alt+z",
    "command": "comment-ts.commentThis"
  },
  // ===================================================================
  // DISABLED DEFAULT KEYBINDINGS
  // ===================================================================
  {
    "key": "ctrl+h",
    "command": "-workbench.action.previousEditor"
  },
  {
    "key": "ctrl+l",
    "command": "-workbench.action.nextEditor"
  },
  {
    "key": "ctrl+h",
    "command": "-workbench.action.previousSideBarView",
    "when": "sideBarFocus"
  },
  {
    "key": "ctrl+l",
    "command": "-workbench.action.nextSideBarView",
    "when": "sideBarFocus"
  },
  {
    "key": "ctrl+shift+g",
    "command": "-workbench.view.scm"
  },
  {
    "key": "shift+ctrl+=",
    "command": "-workbench.action.zoomIn"
  },
  {
    "key": "ctrl+numpad_add",
    "command": "-workbench.action.zoomIn"
  },
  {
    "key": "shift+ctrl+-",
    "command": "-workbench.action.zoomOut"
  },
  {
    "key": "ctrl+numpad_subtract",
    "command": "-workbench.action.zoomOut"
  },
  {
    "key": "ctrl+numpad0",
    "command": "-workbench.action.zoomReset"
  },
  {
    "key": "shift+ctrl+x",
    "command": "-extension.vscode-git-automator.pushLocalCommits"
  },
  {
    "key": "shift+ctrl+t",
    "command": "-workbench.action.reopenClosedEditor"
  },
  {
    "key": "ctrl+`",
    "command": "-workbench.action.terminal.toggleTerminal"
  },
  {
    "key": "shift+ctrl+l",
    "command": "-editor.action.selectHighlights",
    "when": "editorFocus"
  },
  {
    "key": "alt+ctrl+p",
    "command": "-projectManager.listProjects"
  },
  {
    "key": "ctrl+n",
    "command": "-workbench.action.files.newUntitledFile"
  },
  {
    "key": "ctrl+v",
    "command": "-markdown.extension.editing.paste",
    "when": "editorHasSelection && editorTextFocus && editorLangId == 'markdown'"
  },
  {
    "key": "ctrl+b",
    "command": "-markdown.extension.editing.toggleBold",
    "when": "editorTextFocus && !editorReadonly && editorLangId == 'markdown'"
  },
  {
    "key": "ctrl+i",
    "command": "-markdown.extension.editing.toggleItalic",
    "when": "editorTextFocus && !editorReadonly && editorLangId == 'markdown'"
  },
  {
    "key": "ctrl+d",
    "command": "-editor.action.addSelectionToNextFindMatch",
    "when": "editorFocus"
  },
  {
    "key": "ctrl+d",
    "command": "-extension.vim_cmd+d",
    "when": "editorTextFocus && vim.active && vim.use<D-d> && !inDebugRepl"
  },
  {
    "key": "ctrl+f",
    "command": "-cursorRight",
    "when": "textInputFocus"
  },
  {
    "key": "ctrl+m",
    "command": "-markdown.extension.editing.toggleMath",
    "when": "editorTextFocus && !editorReadonly && editorLangId == 'markdown'"
  },
  {
    "key": "ctrl+\\",
    "command": "-workbench.action.terminal.split",
    "when": "terminalFocus"
  },
  {
    "key": "ctrl+shift+5",
    "command": "-workbench.action.terminal.split",
    "when": "terminalFocus"
  },
  {
    "key": "shift+ctrl+;",
    "command": "-breadcrumbs.focus",
    "when": "breadcrumbsPossible"
  },
  {
    "key": "ctrl+shift+`",
    "command": "-workbench.action.terminal.new",
    "when": "!terminalFocus"
  },
  {
    "key": "ctrl+alt+d ctrl+alt+d",
    "command": "-docthis.documentThis"
  },
  {
    "key": "ctrl+alt+c ctrl+alt+c",
    "command": "-comment-ts.commentThis"
  },
  {
    "key": "ctrl+q",
    "command": "-extension.vim_winCtrlQ",
    "when": "editorTextFocus && vim.active && vim.use<C-q> && !inDebugRepl"
  },
  {
    "key": "ctrl+q",
    "command": "-workbench.action.quickOpenView"
  },
  {
    "key": "ctrl+q",
    "command": "-workbench.action.quickOpenNavigateNextInViewPicker",
    "when": "inQuickOpen && inViewsPicker"
  },
  {
    "key": "ctrl+u",
    "command": "-cursorUndo",
    "when": "textInputFocus"
  },
  {
    "key": "ctrl+u",
    "command": "-thunder-client.import-curl",
    "when": "activeWebviewPanelId == 'tc.env-view' || activeWebviewPanelId == 'tc.request-view'"
  },
  {
    "key": "ctrl+t",
    "command": "-composer.createNewComposerTab",
    "when": "composerFocused || activeEditor == 'workbench.editor.composer' || focusedView =~ /^workbench.panel.aichat.view/"
  },
  {
    "key": "ctrl+n",
    "command": "-chatgpt.newChat",
    "when": "chatgpt.supportsNewChatKeyShortcut"
  },
  {
    "key": "ctrl+n",
    "command": "-chatEditor.action.undoHunk",
    "when": "chatEdits.hasEditorModifications && editorFocus && !chatEdits.isRequestInProgress || chatEdits.hasEditorModifications && notebookCellListFocused && !chatEdits.isRequestInProgress"
  },
  {
    "key": "shift+ctrl+s",
    "command": "-workbench.action.openAgentsView",
    "when": "!isInBackgroundComposerWindow"
  },
  {
    "key": "ctrl+k w",
    "command": "-vscodeWorkspaceSwitcher.openWorkspace"
  },
  {
    "key": "ctrl+k ctrl+w",
    "command": "-vscodeWorkspaceSwitcher.openWorkspaceInNewWindow"
  },
  {
    "key": "ctrl+w",
    "command": "-workbench.action.closeWindow"
  },
  {
    "key": "shift+ctrl+l",
    "command": "-antigravity.prioritized.chat.openNewConversation",
    "when": "!terminalFocus"
  },
  {
    "key": "shift+ctrl+a",
    "command": "-amp.commandPalette.show",
    "when": "amp.activated"
  },
  {
    "key": "shift+ctrl+a",
    "command": "-kilo-code.focusChatInput",
    "when": "true"
  },
  {
    "key": "ctrl+n",
    "command": "amp.agent.newThread",
    "when": "amp.primaryShortcuts && amp.agent.active && amp.agent.activeView == 'sidebar'"
  },
  {
    "key": "ctrl+l",
    "command": "-amp.agent.newThread",
    "when": "amp.primaryShortcuts"
  },
  {
    "key": "ctrl+n",
    "command": "chatgpt.newChat",
    "when": "focusedView == 'chatgpt.view'"
  }
]
